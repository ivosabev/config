{
  "$schema": "https://json.schemastore.org/package",
  "name": "@ivosabev/config",
  "version": "2.10.0",
  "description": "Reusable ESLint, Prettier, TypeScript configs.",
  "keywords": [
    "javascript",
    "typescript",
    "ecmascript",
    "eslint",
    "lint",
    "config",
    "prettier"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ivosabev/eslint-config-ivolution.git"
  },
  "author": "Ivo Sabev <ivolution@gmail.com> (http://ivosabev.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ivosabev/eslint-config-ivolution/issues"
  },
  "homepage": "https://github.com/ivosabev/eslint-config-ivolution#readme",
  "engines": {
    "node": ">=16.0.0"
  },
  "main": "index.js",
  "type": "module",
  "imports": {
    "#/*": "./*"
  },
  "exports": {
    ".": "./index.js",
    "./eslint": "./eslint.js",
    "./prettier": "./prettier.js",
    "./typescript/base": "./typescript/base.json",
    "./typescript/build": "./typescript/build.json",
    "./typescript/dom": "./typescript/dom.json",
    "./typescript/node": "./typescript/node.json",
    "./typescript/reset.d.ts": "./typescript/reset.d.ts"
  },
  "dependencies": {
    "@stylistic/eslint-plugin": "5.4.0",
    "@total-typescript/ts-reset": "0.6.1",
    "@typescript-eslint/eslint-plugin": "8.44.1",
    "@typescript-eslint/parser": "8.44.1",
    "@vitest/eslint-plugin": "1.3.12",
    "eslint": "9.36.0",
    "eslint-import-resolver-node": "0.3.9",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-import-x": "4.16.1",
    "eslint-plugin-jest-dom": "5.5.0",
    "eslint-plugin-jsonc": "2.20.1",
    "eslint-plugin-newline-destructuring": "1.2.2",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0-canary-4dff0e62-20241213",
    "eslint-plugin-sort-destructure-keys": "2.0.0",
    "eslint-plugin-sort-keys-fix": "1.1.2",
    "eslint-plugin-tailwindcss": "3.18.2",
    "eslint-plugin-testing-library": "7.9.1",
    "globals": "16.4.0",
    "jsonc-eslint-parser": "2.4.1",
    "ts-plugin-sort-import-suggestions": "1.0.4",
    "typescript-eslint": "8.44.1"
  },
  "devDependencies": {
    "@types/react": "19.1.13",
    "jest": "30.1.3",
    "npm-check": "6.0.1",
    "npm-run-all": "4.1.5",
    "prettier": "3.6.2",
    "prettier-plugin-tailwindcss": "0.6.14",
    "react": "19.1.1",
    "release-it": "19.0.5",
    "typescript": "5.9.2"
  },
  "scripts": {
    "lint-check": "eslint .",
    "lint": "eslint .",
    "release": "pnpm run validate && release-it",
    "typecheck": "tsc",
    "validate": "run-p -l lint typecheck",
    "up": "corepack up && pnpm update -i"
  },
  "packageManager": "pnpm@9.15.9+sha512.68046141893c66fad01c079231128e9afb89ef87e2691d69e4d40eee228988295fd4682181bae55b58418c3a253bde65a505ec7c5f9403ece5cc3cd37dcf2531"
}
