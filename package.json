{
  "$schema": "https://json.schemastore.org/package",
  "name": "@ivosabev/config",
  "version": "2.4.4",
  "description": "Reusable ESLint, Prettier, TypeScript configs.",
  "keywords": [
    "javascript",
    "typescript",
    "ecmascript",
    "eslint",
    "lint",
    "config",
    "prettier"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ivosabev/eslint-config-ivolution.git"
  },
  "author": "Ivo Sabev <ivolution@gmail.com> (http://ivosabev.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ivosabev/eslint-config-ivolution/issues"
  },
  "homepage": "https://github.com/ivosabev/eslint-config-ivolution#readme",
  "engines": {
    "node": ">=16.0.0"
  },
  "main": "index.js",
  "type": "module",
  "imports": {
    "#/*": "./*"
  },
  "exports": {
    ".": "./index.js",
    "./eslint": "./eslint.js",
    "./prettier": "./prettier.js",
    "./typescript/base": "./typescript/base.json",
    "./typescript/build": "./typescript/build.json",
    "./typescript/dom": "./typescript/dom.json",
    "./typescript/node": "./typescript/node.json",
    "./typescript/reset.d.ts": "./typescript/reset.d.ts"
  },
  "prettier": "./prettier.js",
  "dependencies": {
    "@stylistic/eslint-plugin-js": "2.10.1",
    "@total-typescript/ts-reset": "0.6.1",
    "@typescript-eslint/eslint-plugin": "8.12.2",
    "@typescript-eslint/parser": "8.12.2",
    "@vitest/eslint-plugin": "1.1.7",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-node": "0.3.9",
    "eslint-import-resolver-typescript": "3.6.3",
    "eslint-plugin-import-x": "4.4.0",
    "eslint-plugin-jest-dom": "5.4.0",
    "eslint-plugin-react": "7.37.2",
    "eslint-plugin-react-hooks": "5.0.0",
    "eslint-plugin-sort-destructure-keys": "2.0.0",
    "eslint-plugin-sort-keys-fix": "1.1.2",
    "eslint-plugin-tailwindcss": "3.17.5",
    "eslint-plugin-testing-library": "6.4.0",
    "globals": "15.12.0",
    "prettier-plugin-tailwindcss": "0.6.8",
    "typescript-eslint": "8.12.2"
  },
  "devDependencies": {
    "@types/react": "18.3.12",
    "eslint": "9.14.0",
    "jest": "29.7.0",
    "npm-check": "6.0.1",
    "npm-run-all": "4.1.5",
    "prettier": "3.3.3",
    "react": "18.3.1",
    "release-it": "17.10.0",
    "typescript": "5.6.3"
  },
  "scripts": {
    "format-check": "prettier --check .",
    "format": "prettier --write .",
    "lint-check": "eslint .",
    "lint": "eslint .",
    "release": "npm run validate && release-it",
    "typecheck": "tsc",
    "validate": "run-p -l format lint typecheck",
    "up": "npm-check --update -f"
  },
  "packageManager": "pnpm@9.12.2+sha512.22721b3a11f81661ae1ec68ce1a7b879425a1ca5b991c975b074ac220b187ce56c708fe5db69f4c962c989452eee76c82877f4ee80f474cebd61ee13461b6228"
}
